  object qryServidor: TADOQuery
    Active = True
    Connection = DMConexao.conPessoal
    CursorType = ctStatic
    DataSource = dsPessoal
    Parameters = <
      item
        Name = 'idPessoal'
        DataType = ftString
        NumericScale = 255
        Precision = 255
        Size = 5
        Value = '00126'
      end>
    SQL.Strings = (
      'SELECT '
      'ser.idPessoal,'
      'ser.idServidor, '
      'dad.Naturalidade,'
      'car.Descricao as Cargo,'
      'lot.Descricao as Lotacao, '
      'ser.idDesligamento,'
      'des.Descricao as Desligamento'
      ', dad.Pai, dad.Mae'
      ''
      'FROM tbServidor as ser'
      'LEFT JOIN tbDados dad on dad.idPessoal = ser.idPessoal'
      'LEFT JOIN tbCargo car on car.idCargo = ser.idCargo'
      'LEFT JOIN tbLotacao lot on lot.idLotacao = ser.idLotacao'
      
        'LEFT JOIN tbDesligamento des on des.idDesligamento = ser.idDesli' +
        'gamento'
      'WHERE ser.idPessoal =:idPessoal')
    Left = 592
    Top = 8
  end
  object dsServidor: TDataSource
    DataSet = qryServidor
    Left = 624
    Top = 8
  end
  object qryTelefone: TADOQuery
    Connection = DMConexao.conPessoal
    CursorType = ctStatic
    DataSource = dsPessoal
    Parameters = <
      item
        Name = 'idPessoal'
        DataType = ftString
        NumericScale = 255
        Precision = 255
        Size = 5
        Value = '00126'
      end>
    SQL.Strings = (
      'SELECT '
      'tel.idPessoal,'
      'tel.Numero'
      'FROM tbServ_Telefone as tel'
      'WHERE tel.idPessoal =:idPessoal')
    Left = 696
    Top = 8
  end
