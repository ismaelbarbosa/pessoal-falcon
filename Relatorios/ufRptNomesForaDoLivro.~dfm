  object qryPessoal: TADOQuery
    Active = True
    Connection = DMConexao.conPessoal
    CursorType = ctStatic
    Parameters = <>
    SQL.Strings = (
      'SELECT'
      'pes.idPessoal, Nome, CPF, dt_Nascimento,'
      'dad.Naturalidade,'
      'dad.Pai, dad.Mae,'
      'dad.Conjuge,'
      'dad.Endereco,'
      'dad.Cidade,'
      'dad.UF'
      ''
      'FROM  tbPessoal pes'
      'LEFT JOIN tbDados dad on dad.idPessoal = pes.idPessoal'
      'WHERE nome IN'
      '('
